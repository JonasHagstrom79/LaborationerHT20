        <Label Content="Tal 2" HorizontalAlignment="Left" Margin="257,104,0,0" VerticalAlignment="Top"/>
        <Label Content="Heltal" HorizontalAlignment="Left" Margin="108,233,0,0" VerticalAlignment="Top"/>
